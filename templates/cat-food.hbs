<h1>Cat Food</h1>
	{{#each cat_brands}}
		<div class="brand-name">
			<h2>{{name}}</h2>

			<h3>{{breeds}}</h3>
			
			{{#each types}}
				<div class="food-types">
					<h3 class="type">{{type}}</h3>

				{{#each volumes}}
					<p class="name">{{name}} - ${{price}}</p>
				{{/each}}

				</div>
			{{/each}}

		</div>
	{{/each}}

			
			